{
  "name": "WarlockNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ci:bdd:app": "mocha --reporter spec-xunit-file --require dotenv/config --require react-native-mock/mock.js --compilers js:babel-core/register --recursive app/**/*.spec.js",
    "ci": "npm run ci:bdd:app && npm run lint:app",
    "lint:app": "node_modules/.bin/eslint 'app/**/*.js'",
    "bdd:app": "mocha app/**/*.spec.js --reporter nyan --require dotenv/config --require react-native-mock/mock.js --compilers js:babel-core/register --recursive",
    "test": "npm run bdd:app && npm run lint:app"
  },
  "dependencies": {
    "intl": "^1.2.4",
    "ramda": "^0.21.0",
    "react": "=15.0.2",
    "react-dom": "=15.0.2",
    "react-intl": "^2.1.2",
    "react-native": "^0.26.2",
    "react-native-action-button": "^1.1.5",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "seamless-immutable": "^6.1.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^1.8.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "dirty-chai": "^1.2.2",
    "dotenv": "^2.0.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-prefer-bind-operator": "0.0.4",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-react-native": "^1.1.0-beta",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "=15.0.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "spec-xunit-file": "0.0.1-3"
  }
}
