{
  "name": "Warlock",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint:app": "node_modules/.bin/eslint 'app/**/*.js'",
    "bdd:app": "mocha --reporter spec-xunit-file --require dotenv/config --require react-native-mock/mock.js --compilers js:babel-core/register --recursive app/**/*.spec.js",
    "test": "npm run bdd:app && npm run lint:app"
  },
  "dependencies": {
    "ramda": "^0.21.0",
    "react": "^0.14.8",
    "react-native": "^0.24.1",
    "react-native-action-button": "^1.1.4",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1",
    "seamless-immutable": "^6.1.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^1.6.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "dotenv": "^2.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-react-native": "^1.0.0",
    "mocha": "^2.4.5",
    "mocha-junit-reporter": "^1.11.1",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-native-mock": "0.0.7",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "spec-xunit-file": "0.0.1-3"
  }
}
